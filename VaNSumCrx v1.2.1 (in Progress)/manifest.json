{
  "name": "VaNSum",
  "description": "Enjoy VaNSum on brower extension too!",
  "version": "1.2.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/Logo_ColorBorder_16.png",
      "32": "/images/Logo_ColorBorder_32.png",
      "48": "/images/Logo_ColorBorder_48.png",
      "128": "/images/Logo_ColorBorder_128.png"
    }
  },
  "icons": {
    "16": "/images/Logo_ColorBorder_16.png",
    "32": "/images/Logo_ColorBorder_32.png",
    "48": "/images/Logo_ColorBorder_48.png",
    "128": "/images/Logo_ColorBorder_128.png"
  },
  "options_page": "options.html"
}
